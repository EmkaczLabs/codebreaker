{
  "schemaVersion": 1,
  "id": "codebreaker",
  "version": "${version}",
  "name": "codebreaker",
  "description": "AutoCode for CraftPlay network | created by: emkacz",
  "authors": ["emkacz"],
  "contact": {
    "homepage": "https://github.com/EmkaczLabs/codebreaker",
    "sources": "https://github.com/EmkaczLabs/codebreaker"
  },
  "license": "All-Rights-Reserved",
  "icon": "assets/codebreaker/icon.png",
  "environment": "client",
  "entrypoints": {
    "fabric-datagen": [
      "pl.emkacz.codebreaker.client.CodebreakerDataGenerator"
    ],
    "client": [
      "pl.emkacz.codebreaker.client.CodebreakerClient"
    ],
    "main": [
      "pl.emkacz.codebreaker.Codebreaker"
    ]
  },
  "mixins": [
    "codebreaker.mixins.json",
    {
      "config": "codebreaker.client.mixins.json",
      "environment": "client"
    }
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}"
  }
}
